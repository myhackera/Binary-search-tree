import sys
def KthLargestNumber(root, k):

    def klarge(root):
    
        nonlocal k
        nonlocal res
        
        if root:
        
            klarge(root.right)
            k -= 1
            if k == 0:
                res = root.data
                return 
            klarge(root.left)
        
    res = sys.maxsize
    klarge(root)
    return res if res != sys.maxsize else -1
